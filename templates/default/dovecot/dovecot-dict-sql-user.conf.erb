connect = host=<%= @db_host %> port=<%= @db_port %> dbname=<%= @db_name %> user=<%= @db_user %> password=<%= @db_password %>

map {
  pattern = priv/quota/storage
  table = quota2
  username_field = username
  value_field = bytes
}

map {
  pattern = priv/quota/messages
  table = quota2
  username_field = username
  value_field = messages
}
